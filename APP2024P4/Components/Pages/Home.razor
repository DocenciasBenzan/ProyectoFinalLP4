@page "/"

<PageTitle>Home</PageTitle>

<!-- Incluir el CDN de Bootstrap para usar sus clases -->
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet" />

<!-- Contenedor principal -->
<div class="container-fluid">
    <!-- Barra de navegación con el logo enlazado -->
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <a class="navbar-brand" href="">
            <img src="/bootstrap/Black and White Bold Typographic Car Wash Logo.jpg" width="100" height="auto" alt="Logo" />
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="/">Home</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Servicios</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Precios</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">Contacto</a>
                </li>
            </ul>
        </div>
    </nav>

    <!-- Sección de bienvenida con imagen de fondo -->
    <div class="jumbotron text-center" style="background-image: url('https://www.canva.com/design/DAGXJ-IWmns/FcGbBRDdu-bL6edd1d4diw/edit?utm_content=DAGXJ-IWmns&utm_campaign=designshare&utm_medium=link2&utm_source=sharebutton); background-size: cover; color: white;">
        <h1 class="display-4">¡Bienvenido a nuestro Car Wash!</h1>
        <p class="lead">¡Tu auto nunca estuvo tan limpio y brillante!</p>
        <hr class="my-4">
        <p>Reserva ahora y disfruta de un servicio rápido y de calidad.</p>
        <a class="btn btn-primary btn-lg" href="#" role="button">Ver Servicios</a>
    </div>

    <!-- Sección de servicios -->
    <div class="row text-center">
        <div class="col-md-4">
            <div class="card">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQRQM9eY7lDiHWJyXGAHlhgU_UfA0IIUDdK6Q&s" class="card-img-top" alt="Lavado Exterior">
                <div class="card-body">
                    <h5 class="card-title">Lavado Exterior</h5>
                    <p class="card-text">Deja que tu auto brille por fuera con nuestros productos de calidad.</p>
                    <a href="#" class="btn btn-primary">Ver más</a>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card">
                <img src="data:image/jpeg;base64," class="card-img-top" alt="Lavado Interior">
                <div class="card-body">
                    <h5 class="card-title">Lavado Interior</h5>
                    <p class="card-text">La limpieza perfecta para el interior de tu vehículo.</p>
                    <a href="#" class="btn btn-primary">Ver más</a>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="card">
                <img src="https://blog.way.com/wp-content/uploads/2022/02/Waxing-on-car.jpg" class="card-img-top" alt="Pulido y Cera">
                <div class="card-body">
                    <h5 class="card-title">Pulido y Cera</h5>
                    <p class="card-text">Dale un acabado brillante y protector con nuestro servicio de pulido y cera.</p>
                    <a href="#" class="btn btn-primary">Ver más</a>
                </div>
            </div>
        </div>
    </div>

    <!-- Sección de testimonios -->
    <div class="container my-5">
        <h2 class="text-center">Lo que dicen nuestros clientes</h2>
        <div id="testimonialCarousel" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <blockquote class="blockquote text-center">
                        <p class="mb-0">"El mejor servicio de lavado de autos. Mi coche nunca ha quedado tan limpio. ¡Altamente recomendado!"</p>

                        <footer class="blockquote-footer">Juan Pérez</footer>
                    </blockquote>
                </div>
                <div class="carousel-item">
                    <blockquote class="blockquote text-center">
                        <p class="mb-0">"Rápidos, eficientes y de calidad. Mi auto se ve increíble después de cada lavado."</p>

                        <footer class="blockquote-footer">Ana García</footer>
                    </blockquote>
                </div>
            </div>
            <a class="carousel-control-prev" href="#testimonialCarousel" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#testimonialCarousel" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>

<!-- Incluir el CDN de Bootstrap JS y JQuery -->
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/core@2.5.3/dist/umd/popper.min.js"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
